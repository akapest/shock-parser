{
  "name": "shock-parser",
  "version": "1.0.0",
  "description": "shock-parser",
  "main": "main.js",
  "scripts": {
    "predev-server": "cross-env NODE_ENV=development webpack --config webpack.vendor.config.js",
    "dev-server": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --port 3000",
    "dev": "cross-env NODE_ENV=development electron ./",
    "build": "cross-env NODE_ENV=production webpack --progress --profile --colors",
    "start": "cross-env NODE_ENV=production electron ./",
    "package": "npm run build; npm run package-osx; npm run package-win; npm run package-linux;",
    "package-osx": "electron-packager ./ --platform=darwin --arch=x64 --icon=assets/app.icns --out=package --asar=true --overwrite=force",
    "package-win": "electron-packager ./ --platform=win32 --arch=ia32 --icon=assets/app.ico --out=package --asar=true --overwrite=force",
    "package-linux": "electron-packager ./ --platform=linux --arch=ia32 --icon=assets/app.ico --out=package --asar=true --overwrite=force"
  },
  "dependencies": {
    "assert": "^1.4.1",
    "bluebird": "^3.5.0",
    "chart.js": "^2.7.1",
    "cheerio": "^0.22.0",
    "cheerio-advanced-selectors": "^1.1.1",
    "coffeescript": "^2.0.4",
    "debug": "^2.6.8",
    "electron": "^1.7.0",
    "electron-context-menu": "^0.9.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "mongoose": "^5.0.4",
    "querystring": "^0.2.0",
    "request": "^2.81.0",
    "require-dir": "^0.3.2",
    "rollbar": "^2.3.1",
    "tough-cookie": "^2.3.2",
    "v-tooltip": "^2.0.0-rc.31",
    "vue": "^2.3.4",
    "vue-chartjs": "^3.1.0",
    "vue-pagination-2": "^1.3.1",
    "vue-router": "^2.5.3",
    "vue-save-state": "^1.2.0",
    "vue-tabs-component": "^1.4.0",
    "vue-template-compiler": "^2.5.13",
    "winston": "^2.3.1",
    "winston-logrotate": "^1.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.1",
    "coffee-loader": "^0.9.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "electron-inspector": "^0.1.4",
    "electron-packager": "^8.7.1",
    "electron-reload": "^1.2.1",
    "file-loader": "^0.11.2",
    "mocha": "^3.5.0",
    "node-loader": "^0.6.0",
    "node-sass": "^4.7.2",
    "request-debug": "^0.2.0",
    "sass-loader": "^6.0.6",
    "vue-loader": "^12.2.1",
    "webpack": "^2.7.0",
    "webpack-dev-server": "^2.4.5"
  }
}
